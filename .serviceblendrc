services:
  docker: hello
  hello:
    environments:
      staging:
        env:
          HELLO: '8888'
        run:
          version: '3'
          services:
            nginx:
              image: nginx:latest
              ports:
                - 8080:80
  example:
    local: local
    dependsOn:
      - docker
    environments:
      local:
        envMap:
          PORT: HELLO
        run: python -m http.server ${PORT}
